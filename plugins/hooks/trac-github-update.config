# Add this section to .git/config when configuring hooks

[multimailhook]
	administrator = admin@macports.org
	mailingList = macports-changes@lists.macports.org
	replyTo = macports-dev@lists.macports.org
	commitBrowseURL = https://github.com/macports/%(repo_shortname)s/commit/%(id)s
	commitEmailFormat = html
